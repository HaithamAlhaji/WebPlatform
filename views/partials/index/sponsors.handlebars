<link rel="stylesheet" href="/css/{{default "style"}}/index/sponsors.css">

<!-- ======= sponsors Section ======= -->
<section id="sponsors" class="section-with-bg">

    <div class="container" data-aos="fade-up">
        <div class="section-header">
            <h2>
                <i class="fa fa-bolt" aria-hidden="true"></i>

                {{texts "sponsors"}}
            </h2>
            <p>{{texts "sponsors_description"}}</p>

        </div>

        <div class="row no-gutters sponsors-wrap clearfix" data-aos="zoom-in" data-aos-delay="100">
            {{#each sponsors}}
            <div class="col-lg-3 col-md-4 col-xs-6 justify-content-center">
                <div class="supporter-logo">
                    <img src="/uploads/{{image}}" class="img-fluid" alt="">
                </div>
            </div>
            {{/each }}


        </div>

    </div>

</section><!-- End Sponsors Section -->